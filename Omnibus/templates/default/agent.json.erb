{
  "cloudwatch.emitMetrics": true,
  "kinesis.endpoint": "",
  "firehose.endpoint": "",
  "awsAccessKeyId": "<%= @kinesis_access_key %>",
  "awsSecretAccessKey": "<%= @kinesis_secret_key %>",
  
  "flows": [
    {
      "filePattern": "/usr/share/tomcat7/logs/catalina.out",
      "kinesisStream": "<%= @kinesis_stream_name %>",
      "partitionKeyOption": "RANDOM"
    },
    {
      "filePattern": "/usr/share/tomcat7/logs/catalina.out",
      "deliveryStream": "<%= @kinesis_firehose_name %>"
    }
  ]
}
